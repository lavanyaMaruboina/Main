<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>PinCode_LastDigitRemoved__c</fullName>
    <description>This formula field will store pincode by removing one last digit</description>
    <externalId>false</externalId>
    <formula>IF(ISPICKVAL(Product_Type__c, &apos;Tractor&apos;), FLOOR(Pin_Code__c/10) ,0)</formula>
    <label>PinCode_LastDigitRemoved</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
